{
    "version": 2,
    "env":{
        "MY_KEY":"root",
        "SECRET":"1234"

    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
        }
    ]
}